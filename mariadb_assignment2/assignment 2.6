finance_transactions.payment_mode, finance_transactions.payee_type from students inner join
 finance_transactions on  finance_transactions.payee_id = students.id and 
 finance_transactions.payee_type = 'student' where 
 finance_transactions.payment_mode = "online Payment" 
 and students.school_id = 8 group by students.id;
